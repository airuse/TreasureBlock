                "example": {
                  "code": 200,
                  "message": "success",
                  "data": {
                    "id": 1,
                    "name": "Updated API Key",
                    "api_key": "ak_1234567890abcdef",
                    "permissions": ["read", "write"],
                    "is_active": true,
                    "usage_count": 150,
                    "last_used_at": "2023-12-21T10:30:56Z",
                    "expires_at": "2024-12-31T23:59:59Z",
                    "created_at": "2023-12-21T10:30:56Z",
                    "updated_at": "2023-12-21T10:30:56Z"
                  }
                }
              }
            }
          }
        }
      }
    },
