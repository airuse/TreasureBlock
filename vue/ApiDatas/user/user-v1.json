  "paths": {
    "/api/user/api-keys": {
      "post": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "example": {
                  "code": 200,
                  "message": "success",
                  "data": {
                    "id": 1,
                    "name": "My API Key",
                    "api_key": "ak_1234567890abcdef",
                    "secret_key": "sk_1234567890abcdef",
                    "rate_limit": 1000,
                    "expires_at": "2024-12-31T23:59:59Z",
                    "created_at": "2023-12-21T10:30:56Z"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/user/api-keys": {
      "get": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "example": {
                  "code": 200,
                  "message": "success",
                  "data": [
                    {
                      "id": 1,
                      "name": "My API Key",
                      "api_key": "ak_1234567890abcdef",
                      "permissions": ["read", "write"],
                      "is_active": true,
                      "usage_count": 150,
                      "last_used_at": "2023-12-21T10:30:56Z",
                      "expires_at": "2024-12-31T23:59:59Z",
                      "created_at": "2023-12-21T10:30:56Z",
                      "updated_at": "2023-12-21T10:30:56Z"
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/api/user/api-keys/{keyId}": {
